["trigger", ".", {
  "name": "build-go",
  "expression": ["anyof",
    ["pcre", ".(go|tmpl|md)$"],
    ["name", "generated/assets/manifest.json", "wholename"]
  ],
  "command": ["make", "build-go"],
  "append_files": false,
  "stdout": ">>logs/watchman-build.log",
  "stderr": ">>logs/watchman-build.log"
}]